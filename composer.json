{
    "name": "acelaya/mywebsite-expressive",
    "description": "My personal website built on top of Zend Expressive",
    "type": "Project",
    "license": "MIT",
    "keywords": [
        "zend",
        "expressive",
        "microframework",
        "middleware",
        "psr-7",
        "website",
        "acelaya"
    ],
    "authors": [
        {
            "name": "Alejandro Celaya",
            "email": "alejandro@alejandrocelaya.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Acelaya\\Website\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcelayaTest\\Website\\": "tests"
        }
    },
    "require": {
        "php": "^5.6|^7.0",
        "acelaya/expressive-slim-router": "^2.0",
        "doctrine/cache": "^1.4",
        "google/recaptcha": "~1.1",
        "swiftmailer/swiftmailer": "^5.4",
        "twig/twig": "^1.21",
        "zendframework/zend-config": "^2.5",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-twigrenderer": "^1.0",
        "zendframework/zend-i18n": "^2.5",
        "zendframework/zend-inputfilter": "^2.5",
        "zendframework/zend-servicemanager": "^2.6",
        "zendframework/zend-session": "^2.5"
    },
    "require-dev": {
        "filp/whoops": "^1.1",
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "symfony/var-dumper": "^2.7",
        "umpirsky/twig-gettext-extractor": "^1.1",
        "vlucas/phpdotenv": "^2.0"
    }
}
